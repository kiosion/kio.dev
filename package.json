{
  "private": true,
  "version": "0.0.0",
  "devDependencies": {
    "bumpp": "^8.2.1",
    "eslint": "^8.16.0",
    "husky": "^8.0.0",
    "lint-staged": "^13.0.3",
    "prettier": "^2.6.2",
    "prettier-eslint": "^15.0.0",
    "stylelint-config-standard-scss": "^5.0.0",
    "typescript": "^4.6.4"
  },
  "scripts": {
    "prepare": "yarn husky install && chmod ug+x .husky/* && yarn husky add .husky/pre-commit 'yarn lint-staged'"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
