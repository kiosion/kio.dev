{
  "useTabs": false,
  "tabWidth": 2,
  "semi": true,
  "bracketSameLine": true,
  "singleQuote": true,
  "trailingComma": "all",
  "printWidth": 90,
  "bracketSpacing": true,
  "plugins": [
    "./node_modules/prettier-plugin-svelte/plugin.js",
    "./node_modules/prettier-plugin-tailwindcss/dist/index.mjs"
  ],
  "overrides": [{ "files": "*.svelte", "options": { "parser": "svelte" } }]
}
