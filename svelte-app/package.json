{
  "name": "svelte-app",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "vite build --mode production",
    "build:test": "rimraf ./dist && vite build --mode testing",
    "dev": "vite dev --mode development --host",
    "dev:backed": "vite dev --mode backed --host",
    "dev:cypress": "CYPRESS_MODE=dev cypress open",
    "package": "svelte-kit package",
    "preview": "vite preview",
    "serve": "node ./dist",
    "test:vitest": "MODE=testing vitest --run",
    "test:vitest:ui": "MODE=testing vitest --ui",
    "test:cypress": "MODE=testing cypress run",
    "test": "yarn test:vitest; yarn test:cypress",
    "coverage": "vitest run --coverage",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "format": "prettier --write --plugin-search-dir=. ./src/**/*.{ts,svelte}",
    "lint": "yarn format && eslint --fix ./src/**/*.{ts,svelte}"
  },
  "devDependencies": {
    "@cypress/vite-dev-server": "^4.0.1",
    "@iconify/svelte": "^3.0.0",
    "@poppanator/sveltekit-svg": "^1.0.0",
    "@portabletext/svelte": "^1.0.1",
    "@sanity/image-url": "^1.0.1",
    "@sveltejs/adapter-auto": "^1.0.0-next.86",
    "@sveltejs/adapter-netlify": "^1.0.0-next.78",
    "@sveltejs/adapter-node": "^1.0.0-next.100",
    "@sveltejs/kit": "1.0.0-next.529",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/line-clamp": "^0.4.2",
    "@testing-library/cypress": "^8.0.3",
    "@testing-library/svelte": "^3.2.2",
    "@types/cypress": "^1.1.3",
    "@types/randomstring": "^1.1.8",
    "@types/seedrandom": "^3.0.2",
    "@typescript-eslint/eslint-plugin": "^5.42.0",
    "@typescript-eslint/parser": "^5.44.0",
    "@vitest/ui": "^0.24.4",
    "autoprefixer": "^10.4.13",
    "c8": "^7.12.0",
    "cypress": "^10.10.0",
    "cypress-svelte-unit-test": "^3.3.4",
    "devalue": "4.0.1",
    "devtools-detect": "^4.0.0",
    "dotenv": "^16.0.3",
    "eslint": "^8.25.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-svelte3": "^4.0.0",
    "highlight.js": "^11.6.0",
    "jsdom": "^20.0.2",
    "miragejs": "^0.1.45",
    "moment": "^2.29.4",
    "netlify-cli": "^12.0.9",
    "postcss": "^8.4.18",
    "prettier": "^2.6.2",
    "prettier-plugin-svelte": "^2.8.0",
    "randomstring": "^1.2.2",
    "rimraf": "^3.0.2",
    "rollup": "^2.79.1",
    "sass": "^1.55.0",
    "seedrandom": "^3.0.5",
    "svelte": "3.52",
    "svelte-body": "^1.3.6",
    "svelte-breakpoints": "^0.1.0",
    "svelte-check": "^2.9.2",
    "svelte-highlight": "^6.2.1",
    "svelte-loading-spinners": "^0.1.7",
    "svelte-preprocess": "^4.10.6",
    "tailwindcss": "^3.2.1",
    "ts-essentials": "^9.3.0",
    "tslib": "^2.4.1",
    "typescript": "^4.8.4",
    "uifx": "^2.0.7",
    "vite": "^3.1.3",
    "vite-plugin-inspect": "^0.7.5",
    "vite-plugin-test-selectors": "0.0.2",
    "vitest": "^0.24.3"
  },
  "type": "module",
  "dependencies": {
    "pixelarticons": "^1.7.0",
    "twemoji": "^14.0.2"
  },
  "packageManager": "yarn@4.0.0-rc.25"
}
