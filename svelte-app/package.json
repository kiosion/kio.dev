{
  "name": "svelte-app",
  "version": "0.0.1",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "package": "svelte-kit package",
    "preview": "vite preview",
    "prepare": "svelte-kit sync",
    "serve": "node ./dist",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "format": "prettier --config ../.prettierrc.json --write --plugin-search-dir=. .",
    "lint": "yarn format && eslint -c ../.eslintrc --resolve-plugins-relative-to ./node_modules --fix ."
  },
  "devDependencies": {
    "@iconify/svelte": "^2.2.1",
    "@portabletext/svelte": "^1.0.1",
    "@rollup/plugin-replace": "^4.0.0",
    "@sveltejs/adapter-auto": "next",
    "@sveltejs/adapter-node": "^1.0.0-next.83",
    "@sveltejs/kit": "next",
    "@tailwindcss/aspect-ratio": "^0.4.0",
    "@tailwindcss/line-clamp": "^0.4.0",
    "@typescript-eslint/eslint-plugin": "^5.27.0",
    "@typescript-eslint/parser": "^5.27.0",
    "autoprefixer": "^10.4.7",
    "eslint": "^8.16.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-svelte3": "^4.0.0",
    "moment": "^2.29.4",
    "prettier": "^2.6.2",
    "prettier-plugin-svelte": "^2.7.0",
    "sass": "^1.54.0",
    "svelte": "^3.44.0",
    "svelte-body": "^1.3.6",
    "svelte-check": "^2.7.1",
    "svelte-loading-spinners": "^0.1.7",
    "svelte-preprocess": "^4.10.6",
    "tailwindcss": "^3.1.6",
    "tslib": "^2.3.1",
    "typescript": "^4.7.4",
    "vite": "^3.0.0"
  },
  "type": "module"
}
