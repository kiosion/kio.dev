{
  "name": "svelte-app",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "vite build --mode production",
    "build:test": "rimraf ./dist && vite build --mode testing",
    "dev": "vite dev --mode development",
    "dev:backed": "vite dev --mode backed",
    "package": "svelte-kit package",
    "preview": "vite preview",
    "serve": "node ./dist",
    "test:vitest": "MODE=testing vitest --run",
    "test:cypress": "MODE=testing cypress run",
    "test": "yarn test:vitest; yarn test:cypress",
    "coverage": "vitest run --coverage",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "format": "prettier --write --plugin-search-dir=. ./src/**/*.{ts,svelte}",
    "lint": "yarn format && eslint --fix ./src/**/*.{ts,svelte}"
  },
  "devDependencies": {
    "@cypress/vite-dev-server": "^3.1.1",
    "@iconify/svelte": "^2.2.1",
    "@portabletext/svelte": "^1.0.1",
    "@sveltejs/adapter-auto": "next",
    "@sveltejs/adapter-netlify": "^1.0.0-next.71",
    "@sveltejs/adapter-node": "^1.0.0-next.86",
    "@sveltejs/kit": "next",
    "@tailwindcss/aspect-ratio": "^0.4.0",
    "@tailwindcss/line-clamp": "^0.4.0",
    "@testing-library/cypress": "^8.0.3",
    "@testing-library/svelte": "^3.1.3",
    "@types/randomstring": "^1.1.8",
    "@typescript-eslint/eslint-plugin": "^5.33.1",
    "@typescript-eslint/parser": "^5.33.1",
    "@vitest/ui": "^0.22.0",
    "autoprefixer": "^10.4.8",
    "c8": "^7.12.0",
    "cypress": "^10.6.0",
    "cypress-svelte-unit-test": "^3.3.4",
    "dotenv": "^16.0.1",
    "eslint": "^8.22.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-svelte3": "^4.0.0",
    "jsdom": "^20.0.0",
    "miragejs": "^0.1.45",
    "moment": "^2.29.4",
    "netlify-cli": "^10.17.8",
    "postcss": "^8.4.16",
    "prettier": "^2.6.2",
    "prettier-plugin-svelte": "^2.7.0",
    "randomstring": "^1.2.2",
    "rimraf": "^3.0.2",
    "rollup": "^2.78.0",
    "sass": "^1.54.4",
    "svelte": "^3.44.0",
    "svelte-body": "^1.3.6",
    "svelte-check": "^2.7.1",
    "svelte-loading-spinners": "^0.1.7",
    "svelte-preprocess": "^4.10.6",
    "tailwindcss": "^3.1.8",
    "tslib": "^2.3.1",
    "twemoji": "^14.0.2",
    "typescript": "^4.7.4",
    "vite": "^3.0.8",
    "vite-plugin-inspect": "^0.6.0",
    "vite-plugin-test-selectors": "0.0.2",
    "vitest": "^0.22.1"
  },
  "type": "module"
}
