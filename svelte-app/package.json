{
  "name": "svelte-app",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "vite build --mode production",
    "build:test": "rm -rf ./dist && vite build --mode testing",
    "dev": "vite dev --mode development --host",
    "dev:backed": "vite dev --mode backed --host",
    "package": "svelte-kit package",
    "preview": "vite preview",
    "generate": "bun ./scripts/generate-types && pnpm eslint --fix \"./types/**/*.ts\"",
    "test": "test:vitest; test:playwright",
    "test:vitest": "MODE=testing vitest --run",
    "test:playwright": "playwright test",
    "coverage": "vitest run --coverage",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "format": "prettier --write ./src/**/*.{ts,svelte}",
    "format:json": "bun ./scripts/lint-json.ts ./src/languages/en.json ./src/languages/fr.json",
    "lint": "pnpm format && pnpm eslint --fix \"./src/**/*.{ts,svelte}\" \"./types/**/*.ts\"",
    "lint:check": "pnpm prettier --check ./src/**/*.{ts,svelte} && pnpm eslint \"./src/**/*.{ts,svelte}\" \"./types/**/*.ts\""
  },
  "devDependencies": {
    "@eslint/js": "^9.29.0",
    "@floating-ui/dom": "^1.7.1",
    "@playwright/test": "^1.53.1",
    "@portabletext/svelte": "^3.0.0",
    "@portabletext/toolkit": "^2.0.17",
    "@portabletext/types": "^2.0.13",
    "@sanity/client": "^7.6.0",
    "@sanity/image-url": "^2.0.3",
    "@stylistic/eslint-plugin": "^5.7.0",
    "@sveltejs/adapter-netlify": "5.0.2",
    "@sveltejs/adapter-node": "5.2.12",
    "@sveltejs/kit": "^2.49.5",
    "@sveltejs/vite-plugin-svelte": "^5.1.0",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/postcss": "^4.1.10",
    "@tailwindcss/vite": "^4.1.18",
    "@typescript-eslint/eslint-plugin": "^8.35.0",
    "@typescript-eslint/parser": "^8.35.0",
    "autoprefixer": "^10.4.21",
    "dotenv": "^16.5.0",
    "eslint": "^9.29.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-local-rules": "^3.0.2",
    "eslint-plugin-prettier": "^5.5.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-svelte": "^3.9.3",
    "eslint-plugin-unused-imports": "^4.1.4",
    "groq": "^3.94.2",
    "highlight.js": "11.11.1",
    "jsdom": "^24.1.0",
    "netlify-cli": "^22.1.6",
    "postcss": "^8.5.6",
    "postcss-load-config": "^5.1.0",
    "prettier": "^3.6.0",
    "prettier-plugin-svelte": "^3.4.0",
    "prettier-plugin-tailwindcss": "^0.6.13",
    "sass": "^1.77.6",
    "svelte": "^5.34.8",
    "svelte-body": "^1.4.0",
    "svelte-breakpoints": "^0.3.0",
    "svelte-check": "^3.7.1",
    "svelte-eslint-parser": "^1.2.0",
    "svelte-highlight": "^7.8.3",
    "tailwindcss": "^4.1.10",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.35.0",
    "vite": "^7.3.1",
    "vite-plugin-inspect": "^11.2.0",
    "vitest": "^3.2.4"
  },
  "type": "module"
}
