{
  "name": "svelte-app",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "vite build --mode production",
    "build:test": "rm -rf ./dist && vite build --mode testing",
    "dev": "vite dev --mode development --host",
    "dev:backed": "vite dev --mode backed --host",
    "package": "svelte-kit package",
    "preview": "vite preview",
    "generate": "bun ./scripts/generate-types && pnpm eslint --fix \"./types/**/*.ts\"",
    "test": "test:vitest; test:playwright",
    "test:vitest": "MODE=testing vitest --run",
    "test:playwright": "playwright test",
    "coverage": "vitest run --coverage",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "format": "prettier --write ./src/**/*.{ts,svelte}",
    "format:json": "bun ./scripts/lint-json.ts ./src/languages/en.json ./src/languages/fr.json",
    "lint": "pnpm format && pnpm eslint --fix \"./src/**/*.{ts,svelte}\" \"./types/**/*.ts\""
  },
  "devDependencies": {
    "@floating-ui/dom": "^1.6.12",
    "@playwright/test": "1.44.1",
    "@portabletext/svelte": "2.1.11",
    "@portabletext/toolkit": "2.0.15",
    "@portabletext/types": "2.0.13",
    "@sanity/image-url": "1.1.0",
    "@sveltejs/adapter-netlify": "4.3.6",
    "@sveltejs/adapter-node": "5.2.9",
    "@sveltejs/kit": "2.8.5",
    "@sveltejs/vite-plugin-svelte": "3.1.1",
    "@tailwindcss/aspect-ratio": "0.4.2",
    "autoprefixer": "10.4.20",
    "dotenv": "16.4.5",
    "eslint": "9.15.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-local-rules": "^3.0.2",
    "eslint-plugin-prettier": "5.2.1",
    "eslint-plugin-simple-import-sort": "12.1.1",
    "eslint-plugin-svelte": "2.46.0",
    "eslint-plugin-unused-imports": "4.1.4",
    "highlight.js": "11.10.0",
    "jsdom": "25.0.1",
    "netlify-cli": "17.37.2",
    "postcss": "8.4.49",
    "postcss-load-config": "6.0.1",
    "prettier": "3.4.1",
    "prettier-plugin-svelte": "3.3.2",
    "prettier-plugin-tailwindcss": "0.6.9",
    "sass": "1.81.0",
    "svelte": "5.2.10",
    "svelte-body": "2.0.0",
    "svelte-breakpoints": "0.3.0",
    "svelte-check": "4.1.0",
    "svelte-eslint-parser": "0.43.0",
    "svelte-highlight": "7.6.1",
    "tailwindcss": "3.4.15",
    "typescript": "5.7.2",
    "typescript-eslint": "^8.16.0",
    "vite": "5.2.10",
    "vite-plugin-inspect": "0.10.1",
    "vite-plugin-test-selectors": "0.0.2",
    "vitest": "1.6.0"
  },
  "type": "module"
}
