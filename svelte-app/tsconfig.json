{
  "extends": "./.svelte-kit/tsconfig.json",
  "compilerOptions": {
    "allowJs": true,
    "baseUrl": ".",
    "checkJs": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "noImplicitAny": true,
    "noImplicitThis": true,
    "paths": {
      "@/*": ["src/*"],
      "@types": ["types.d"],
      "$lib": ["src/lib"],
      "$lib/*": ["src/lib/*"]
    },
    "removeComments": true,
    "rootDir": "./",
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "sourceMap": true,
    "strict": true
  }
}
